{
  "name": "synaptik",
  "version": "0.6.0",
  "description": "Yet another state management library",
  "main": "lib/synaptik.js",
  "module": "lib/synaptik.es.js",
  "repository": "https://github.com/kylealwyn/synaptik",
  "author": "Kyle Alwyn <kjalwyn@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "logger.js",
    "README.md"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "bump": "standard-version",
    "format": "prettier --write {src,example}/**/*.js",
    "test": "jest",
    "build": "npm-run-all clean rollup",
    "clean": "rimraf dist",
    "rollup": "rollup -c",
    "release": "npm-run-all test build bump && git push --follow-tags origin master && npm publish",
    "example": "parcel example/index.html"
  },
  "dependencies": {
    "create-react-context": "^0.2.3",
    "deep-object-diff": "^1.1.0",
    "react-fast-compare": "^2.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.1.2",
    "immer": "^1.7.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^4.0.1",
    "standard-version": "^4.4.0"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
