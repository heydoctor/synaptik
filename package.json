{
  "name": "synaptik",
  "version": "1.2.3",
  "description": "Yet another state management library",
  "main": "lib/synaptik.js",
  "module": "lib/synaptik.es.js",
  "repository": "https://github.com/sappira-inc/synaptik",
  "author": "Kyle Alwyn <kjalwyn@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "logger.js",
    "README.md"
  ],
  "scripts": {
    "bump": "standard-version",
    "format": "prettier --write {src,example}/**/*.js",
    "test": "jest",
    "build": "npm-run-all clean rollup",
    "clean": "rimraf dist",
    "rollup": "rollup -c",
    "release": "npm-run-all test build bump",
    "example": "parcel example/index.html"
  },
  "dependencies": {
    "deep-object-diff": "^1.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "husky": "^2.7.0",
    "immer": "^3.1.3",
    "jest": "^24.1.0",
    "jest-dom": "^3.1.2",
    "lint-staged": "^8.1.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-test-renderer": "^16.8.3",
    "react-testing-library": "^8.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-filesize": "^6.0.1",
    "standard-version": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js}": [
      "prettier --write",
      "git add"
    ]
  }
}
