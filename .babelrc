{
  "comments": false,
  "presets": [
    [
      "@babel/env",
      {
        "loose": true,
        "targets": {
          "browsers": ["last 1 version"]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  }
}
